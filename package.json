{
  "name": "si18n.js",
  "version": "1.0.1",
  "description": "A simple way to integrate internationalization on a small web site project.",
  "main": "./si18n.js",
  "directories": {
    "src": "src",
    "site": "website"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "minify-si18n-npm": "terser ./src/si18n.js --output ./si18n.js --compress --mangle --module --ecma 2016",
    "minify-si18n-demo": "terser ./src/si18n.js --output ./website/si18n.js --compress --mangle --module --ecma 2016",
    "minify-demo": "terser ./website/demo.js --output ./website/demo.min.js --compress --mangle --ecma 2016",
    "build": "npm run minify-si18n-npm && npm run minify-si18n-demo && npm run minify-demo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jdbruxelles/si18n.git"
  },
  "keywords": [
    "i18n",
    "translation",
    "localisation",
    "internationalization",
    "website"
  ],
  "author": "Jos√© dBruxelles (https://github.com/jdbruxelles)",
  "license": "MIT",
  "funding": [
    {
